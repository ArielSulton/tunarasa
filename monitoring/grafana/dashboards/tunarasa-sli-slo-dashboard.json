{
  "dashboard": {
    "id": null,
    "title": "Tunarasa SLI/SLO Dashboard",
    "tags": ["tunarasa", "sli", "slo", "business"],
    "timezone": "browser",
    "panels": [
      {
        "id": 1,
        "title": "Service Availability SLI",
        "type": "stat",
        "targets": [
          {
            "expr": "tunarasa:sli:availability:rate5m",
            "legendFormat": "Availability %"
          }
        ],
        "fieldConfig": {
          "defaults": {
            "color": {
              "mode": "thresholds"
            },
            "thresholds": {
              "mode": "absolute",
              "steps": [
                {
                  "color": "red",
                  "value": 0
                },
                {
                  "color": "yellow",
                  "value": 99.5
                },
                {
                  "color": "green",
                  "value": 99.9
                }
              ]
            },
            "unit": "percent"
          }
        },
        "gridPos": {
          "h": 8,
          "w": 6,
          "x": 0,
          "y": 0
        }
      },
      {
        "id": 2,
        "title": "Response Latency SLI (P95)",
        "type": "stat",
        "targets": [
          {
            "expr": "tunarasa:sli:latency:p95",
            "legendFormat": "P95 Latency"
          }
        ],
        "fieldConfig": {
          "defaults": {
            "color": {
              "mode": "thresholds"
            },
            "thresholds": {
              "mode": "absolute",
              "steps": [
                {
                  "color": "green",
                  "value": 0
                },
                {
                  "color": "yellow",
                  "value": 0.5
                },
                {
                  "color": "red",
                  "value": 1
                }
              ]
            },
            "unit": "s"
          }
        },
        "gridPos": {
          "h": 8,
          "w": 6,
          "x": 6,
          "y": 0
        }
      },
      {
        "id": 3,
        "title": "Error Rate SLI",
        "type": "stat",
        "targets": [
          {
            "expr": "tunarasa:sli:error_rate:rate5m",
            "legendFormat": "Error Rate %"
          }
        ],
        "fieldConfig": {
          "defaults": {
            "color": {
              "mode": "thresholds"
            },
            "thresholds": {
              "mode": "absolute",
              "steps": [
                {
                  "color": "green",
                  "value": 0
                },
                {
                  "color": "yellow",
                  "value": 0.1
                },
                {
                  "color": "red",
                  "value": 1
                }
              ]
            },
            "unit": "percent"
          }
        },
        "gridPos": {
          "h": 8,
          "w": 6,
          "x": 12,
          "y": 0
        }
      },
      {
        "id": 4,
        "title": "Request Throughput SLI",
        "type": "stat",
        "targets": [
          {
            "expr": "tunarasa:sli:throughput:rate5m",
            "legendFormat": "Requests/sec"
          }
        ],
        "fieldConfig": {
          "defaults": {
            "color": {
              "mode": "palette-classic"
            },
            "unit": "reqps"
          }
        },
        "gridPos": {
          "h": 8,
          "w": 6,
          "x": 18,
          "y": 0
        }
      },
      {
        "id": 5,
        "title": "Daily Active Users",
        "type": "stat",
        "targets": [
          {
            "expr": "count(count by (session_id) (tunarasa_gesture_requests_total[24h]))",
            "legendFormat": "DAU"
          }
        ],
        "fieldConfig": {
          "defaults": {
            "color": {
              "mode": "thresholds"
            },
            "thresholds": {
              "mode": "absolute",
              "steps": [
                {
                  "color": "red",
                  "value": 0
                },
                {
                  "color": "yellow",
                  "value": 5
                },
                {
                  "color": "green",
                  "value": 10
                }
              ]
            },
            "unit": "short"
          }
        },
        "gridPos": {
          "h": 8,
          "w": 6,
          "x": 0,
          "y": 8
        }
      },
      {
        "id": 6,
        "title": "Average Session Duration",
        "type": "stat",
        "targets": [
          {
            "expr": "avg(tunarasa_session_duration_seconds)",
            "legendFormat": "Avg Duration"
          }
        ],
        "fieldConfig": {
          "defaults": {
            "color": {
              "mode": "thresholds"
            },
            "thresholds": {
              "mode": "absolute",
              "steps": [
                {
                  "color": "red",
                  "value": 0
                },
                {
                  "color": "yellow",
                  "value": 30
                },
                {
                  "color": "green",
                  "value": 60
                }
              ]
            },
            "unit": "s"
          }
        },
        "gridPos": {
          "h": 8,
          "w": 6,
          "x": 6,
          "y": 8
        }
      },
      {
        "id": 7,
        "title": "Gesture Recognition Accuracy",
        "type": "stat",
        "targets": [
          {
            "expr": "avg(tunarasa_gesture_confidence_score)",
            "legendFormat": "Accuracy Score"
          }
        ],
        "fieldConfig": {
          "defaults": {
            "color": {
              "mode": "thresholds"
            },
            "thresholds": {
              "mode": "absolute",
              "steps": [
                {
                  "color": "red",
                  "value": 0
                },
                {
                  "color": "yellow",
                  "value": 0.7
                },
                {
                  "color": "green",
                  "value": 0.8
                }
              ]
            },
            "unit": "percentunit"
          }
        },
        "gridPos": {
          "h": 8,
          "w": 6,
          "x": 12,
          "y": 8
        }
      },
      {
        "id": 8,
        "title": "AI Response Quality",
        "type": "stat",
        "targets": [
          {
            "expr": "avg(tunarasa_ai_quality_score)",
            "legendFormat": "AI Quality Score"
          }
        ],
        "fieldConfig": {
          "defaults": {
            "color": {
              "mode": "thresholds"
            },
            "thresholds": {
              "mode": "absolute",
              "steps": [
                {
                  "color": "red",
                  "value": 0
                },
                {
                  "color": "yellow",
                  "value": 0.7
                },
                {
                  "color": "green",
                  "value": 0.8
                }
              ]
            },
            "unit": "percentunit"
          }
        },
        "gridPos": {
          "h": 8,
          "w": 6,
          "x": 18,
          "y": 8
        }
      },
      {
        "id": 9,
        "title": "SLI Trends",
        "type": "graph",
        "targets": [
          {
            "expr": "tunarasa:sli:availability:rate5m",
            "legendFormat": "Availability %"
          },
          {
            "expr": "tunarasa:sli:error_rate:rate5m",
            "legendFormat": "Error Rate %"
          },
          {
            "expr": "tunarasa:sli:latency:p95 * 1000",
            "legendFormat": "P95 Latency (ms)"
          }
        ],
        "fieldConfig": {
          "defaults": {
            "color": {
              "mode": "palette-classic"
            }
          }
        },
        "gridPos": {
          "h": 8,
          "w": 12,
          "x": 0,
          "y": 16
        }
      },
      {
        "id": 10,
        "title": "Infrastructure Metrics",
        "type": "graph",
        "targets": [
          {
            "expr": "(sum(tunarasa_db_connections_active) / sum(tunarasa_db_connections_max)) * 100",
            "legendFormat": "DB Connection Pool %"
          },
          {
            "expr": "(redis_memory_used_bytes / redis_memory_max_bytes) * 100",
            "legendFormat": "Redis Memory Usage %"
          }
        ],
        "fieldConfig": {
          "defaults": {
            "color": {
              "mode": "palette-classic"
            },
            "unit": "percent"
          }
        },
        "gridPos": {
          "h": 8,
          "w": 12,
          "x": 12,
          "y": 16
        }
      }
    ],
    "time": {
      "from": "now-1h",
      "to": "now"
    },
    "refresh": "30s"
  }
}
